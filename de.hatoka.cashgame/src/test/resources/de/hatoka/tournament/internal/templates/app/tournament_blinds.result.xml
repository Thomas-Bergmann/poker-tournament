<form xmlns="http://www.w3.org/1999/xhtml" action="actionList" method="POST">
<table class="table table-striped">
<tr>
<th>Select</th>
<th>Small Blind</th>
<th>Big Blind</th>
<th>Ante</th>
<th>Re-Buy</th>
<th>Duration (min)</th>
<th>IsActive</th>
<th>est. Start</th>
<th>est. End</th>
</tr>
<tr>
<td>
<label>
<input type="checkbox" value="1" name="levelID" class="checkbox"/>
</label>
</td>
<td>50</td>
<td>100</td>
<td>-</td>
<td><input type="checkbox" value="1" name="reBuy" class="checkbox" checked="checked" disabled="disabled" /></td>
<td>0:30</td>
<td><input type="checkbox" value="1" name="isActive" class="checkbox" checked="checked" disabled="disabled" /></td>
<td>6:05 PM</td>
<td>6:35 PM</td>
</tr>
<tr>
<td>
<label>
<input type="checkbox" value="2" name="levelID" class="checkbox"/>
</label>
</td>
<td>100</td>
<td>200</td>
<td>-</td>
<td><input type="checkbox" value="2" name="reBuy" class="checkbox" checked="checked" disabled="disabled" /></td>
<td>0:30</td>
<td><input type="checkbox" value="2" name="isActive" class="checkbox" disabled="disabled" /></td>
<td>6:35 PM</td>
<td>7:05 PM</td>
</tr>
<tr>
<td>
<label>
<input type="checkbox" value="3" name="levelID" class="checkbox"/>
</label>
</td>
<td colspan="3">Pause</td>
<td><input type="checkbox" value="3" name="reBuy" class="checkbox" checked="checked" disabled="disabled" /></td>
<td>0:10</td>
<td><input type="checkbox" value="3" name="isActive" class="checkbox" disabled="disabled" /></td>
<td>7:05 PM</td>
<td>7:15 PM</td>
</tr>
<tr>
<td>
<label>
<input type="checkbox" value="4" name="levelID" class="checkbox"/>
</label>
</td>
<td>250</td>
<td>500</td>
<td>-</td>
<td><input type="checkbox" value="4" name="reBuy" class="checkbox" disabled="disabled" /></td>
<td>0:30</td>
<td><input type="checkbox" value="4" name="isActive" class="checkbox" disabled="disabled" /></td>
<td>7:15 PM</td>
<td>7:45 PM</td>
</tr>
<tr>
<td/>
<td>
<label class="sr-only" for="inputsmallBlind">Small Blind</label>
<input placeholder="Small Blind" value="500" name="smallBlind" type="text" id="inputsmallBlind" class="form-control"/>
</td>
<td>
<label class="sr-only" for="inputbigBlind">Big Blind</label>
<input placeholder="Big Blind" value="1000" name="bigBlind" type="text" id="inputbigBlind" class="form-control"/>
</td>
<td>
<label class="sr-only" for="inputante">Ante</label>
<input placeholder="Ante" value="0" name="ante" type="text" id="inputante" class="form-control"/>
</td>
<td></td>
<td>
<label class="sr-only" for="inputduration">Duration</label>
<input placeholder="Duration" value="30" name="duration" type="text" id="inputduration" class="form-control"/>
</td>
<td></td>
<td>7:45 PM</td>
<td>8:15 PM</td>
</tr>
</table>
<button name="create" type="submit" class="btn btn-primary">Add Level</button>
<button name="start" type="submit" class="btn btn-primary">Start Level</button>
<button name="delete" type="submit" class="btn">Delete</button>
<button name="pause" type="submit" class="btn">Add Pause</button>
<button name="enableReBuy" type="submit" class="btn">Enable Re-Buy</button>
<button name="disableReBuy" type="submit" class="btn">Disable Re-Buy</button>
</form>
